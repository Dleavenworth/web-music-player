{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@mui/icons-material": "^5.0.3",
    "@mui/material": "^5.0.3",
    "@mui/styles": "^5.2.2",
    "@mui/x-data-grid": "^5.0.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.3.0",
    "concurrently": "^6.4.0",
    "cross-env": "^7.0.3",
    "electron": "^16.0.3",
    "electron-forge": "^5.2.4",
    "electron-is-dev": "^2.0.0",
    "id3js": "^2.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0",
    "react-scripts": "4.0.3",
    "wait-on": "^6.0.0",
    "web-vitals": "^2.1.2"
  },
  "scripts": {
    "start": "node server/server.js",
    "dev-start": "cross-env PORT=3000 react-scripts start",
    "build": "react-scripts build && (cd server && npm i)",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "concurrently \\\"npm:startServer\\\" \\\"npm:start\\\" ",
    "electrondev": "concurrently -k \\\"cross-env BROWSER=none npm start\\\" \\\"npm:electron\\\" \\\"npm:startServer \\\" ",
    "electron": "wait-on tcp:3000 && electron .",
    "startServer": "npm start --prefix server"
  },
  "main": "public/electron.js",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:5000"
}
